<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="sms_brandname_tree_view_inherit" model="ir.ui.view">
        <field name="name">sms.brandname.inherit.form</field>
        <field name="model">sms.brandname</field>
        <field name="inherit_id" ref="vihat_esms_service.sms_brandname_tree_view" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="website_id" />
            </xpath>
        </field>
    </record>

    <record id="sms_brandname_view_form_inherit" model="ir.ui.view">
        <field name="name">sms.brandname.inherit.form</field>
        <field name="model">sms.brandname</field>
        <field name="inherit_id" ref="vihat_esms_service.sms_brandname_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='create_date']" position="before">
                <field name="website_id" />
            </xpath>
        </field>
    </record>
</odoo>
