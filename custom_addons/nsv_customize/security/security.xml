<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="expense_account_move_rule" model="ir.rule">
            <field name="name">My Expense Journal</field>
            <field name="model_id" ref="account.model_account_move" />
            <field name="domain_force">[(1,'=',1)]</field>
            <field
                name="groups"
                eval="[(4, ref('hr_expense.group_hr_expense_team_approver'))]"
            />
            <field name="perm_read" eval="True" />
            <field name="perm_write" eval="False" />
            <field name="perm_create" eval="False" />
            <field name="perm_unlink" eval="False" />
        </record>
        <record id="group_not_deleted_attachment" model="res.groups">
            <field name="name">Do not delete documents</field>
            <field name="category_id" ref="base.module_category_usability" />
        </record>
    </data>
</odoo>
