<?xml version="1.0" ?>
<odoo>
    <record id="view_matter_concern_tree_view" model="ir.ui.view">
        <field name="name">partner.hobby.tree.view</field>
        <field name="model">matter.concern</field>
        <field name="arch" type="xml">
            <list string="Matter of Concern" editable="bottom">
                <field name="name" />
            </list>
        </field>
    </record>

    <record id="action_open_matter_concern" model="ir.actions.act_window">
        <field name="name">Matter of Concern</field>
        <field name="res_model">matter.concern</field>
        <field name="view_mode">list</field>
        <field name="context" />
    </record>

    <menuitem
        id="matter_concern_config"
        name="Matter of Concern"
        parent="contacts.res_partner_menu_config"
        sequence="8"
        action="action_open_matter_concern"
    />
</odoo>
