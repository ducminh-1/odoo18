<?xml version="1.0" ?>
<odoo>
    <record id="view_partner_hobby_tree_view" model="ir.ui.view">
        <field name="name">partner.hobby.tree.view</field>
        <field name="model">partner.hobby</field>
        <field name="arch" type="xml">
            <list string="Partner Hobby" editable="bottom">
                <field name="name" />
            </list>
        </field>
    </record>

    <record id="action_open_partner_hobby" model="ir.actions.act_window">
        <field name="name">Partner Hobby</field>
        <field name="res_model">partner.hobby</field>
        <field name="view_mode">list</field>
        <field name="context" />
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Partner Hobby
            </p>
        </field>
    </record>

    <menuitem
        id="partner_hobby_config"
        name="Partner Hobby"
        parent="contacts.res_partner_menu_config"
        sequence="7"
        action="action_open_partner_hobby"
    />
</odoo>
